	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Frontend",
			"request": "launch",
			"runtimeArgs": [
				"run",
				"dev",
				"--prefix",
				"pda-frontend"
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		},
		{
			"name": "Run Backend",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/pda-backend/app.py",
			"jinja": true,
			"justMyCode": true
		}
	]
}
